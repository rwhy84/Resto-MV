<form method="post" action="{{ path('upload_delete', {'id': upload.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input name="_method" type="hidden" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ upload.id) }}">
    <button class="btn btnDel">
        <i class="fas fa-trash-alt"></i>
    </button>
</form>
